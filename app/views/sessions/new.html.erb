        <h1>Welcome !!!</h1>
        <%= form_tag sessions_path do %>
          <p>
            <%= label_tag :email %>
            <%= text_field_tag :email, params[:email] %>
          </p>
          <p>
            <%= label_tag :password %>
            <%= password_field_tag :password %>
          </p>
          <p>
            <%= label_tag("FB", "FB") %>
            <%= radio_button_tag("client", "FB") %>
            </p>
            <p>
            <%= label_tag("GITHUB", "GITHUB") %>
            <%= radio_button_tag("client", "GITHUB") %>
            </p>
            <p>
            <%= label_tag("GMAIL", "GMAIL") %>
            <%= radio_button_tag("client", "GMAIL") %>
            </p>
            <p>
            <%= label_tag("TWITTER", "TWITTER") %>
            <%= radio_button_tag("client", "TWITTER") %>
          </p>
          <p class="button"><%= submit_tag "Log in" %></p>
        <% end %>
