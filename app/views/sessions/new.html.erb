<h1>Welcome !!!</h1>
<%= form_tag sessions_path do %>
    <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= radio_button_tag("FB", "FB") %>
    <%= label_tag("FB", "FB") %>
    <%= radio_button_tag("GITHUB", "GITHUB") %>
    <%= label_tag("GITHUB", "GITHUB") %>
    <%= radio_button_tag("GMAIL", "GMAIL") %>
    <%= label_tag("GMAIL", "GMAIL") %>
    <%= radio_button_tag("TWITTER", "TWITTER") %>
    <%= label_tag("TWITTER", "TWITTER") %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>
